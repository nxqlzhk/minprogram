.info-box button {
  border: none;
  padding: 0;
  margin: 0;
  font-size: 28rpx;
  font-weight: 400;
  line-height: 110rpx;
  background-color: #fff;
}
.info-box button::after {
  border: none;
}
.container {
  position: relative;
}
.container .btn-box {
  position: absolute;
  top: 300rpx;
  left: 50%;
  transform: translateX(-50%);
}
.container .btn-box .getUserBtn {
  background-color: #F8F8FF;
  border-radius: 20rpx;
}
.container .info-box {
  display: flex;
  position: relative;
  top: 100rpx;
  height: 600rpx;
  font-size: 28rpx;
  line-height: 88rpx;
  color: black;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}
.container .info-box .userinfo-avatar {
  width: 160rpx;
  height: 160rpx;
  border-radius: 50%;
}
.container .info-box .userinfo-nickname {
  flex: 1;
}
.container .info-box .order {
  flex: 1;
  background-color: #fff;
}
.container .info-box .online {
  flex: 1;
}
.container .info-box .suggest {
  flex: 1;
}
.container .info-box .iphone {
  flex: 1;
}
